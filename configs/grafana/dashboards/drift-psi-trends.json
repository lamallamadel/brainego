      "targets": [
        {
          "datasource": {
            "type": "postgres",
            "uid": "PostgreSQL"
          },
          "editorMode": "code",
          "format": "time_series",
          "rawQuery": true,
          "rawSql": "WITH daily_counts AS (\n  SELECT\n    DATE(created_at) AS day,\n    intent,\n    COUNT(*)::float AS cnt\n  FROM feedback\n  WHERE intent IS NOT NULL\n    AND $__timeFilter(created_at)\n  GROUP BY DATE(created_at), intent\n),\ndaily_dist AS (\n  SELECT\n    day,\n    intent,\n    cnt / NULLIF(SUM(cnt) OVER (PARTITION BY day), 0) AS day_pct\n  FROM daily_counts\n),\noverall_dist AS (\n  SELECT\n    intent,\n    SUM(cnt) / NULLIF(SUM(SUM(cnt)) OVER (), 0) AS baseline_pct\n  FROM daily_counts\n  GROUP BY intent\n)\nSELECT\n  dd.day AS time,\n  dd.intent AS metric,\n  (dd.day_pct - od.baseline_pct) * LN(\n    GREATEST(dd.day_pct, 0.000001) / GREATEST(od.baseline_pct, 0.000001)\n  ) AS value\nFROM daily_dist dd\nJOIN overall_dist od ON dd.intent = od.intent\nORDER BY dd.day, dd.intent",
          "refId": "A",
          "sql": {
            "columns": [
              {
                "parameters": [
                  "value"
                ],
                "type": "function"
              }
            ],
            "groupBy": [
              {
                "property": {
                  "type": "string"
                },
                "type": "groupBy"
              }
            ],
            "limit": 50
          }
        }
      ],
      "title": "PSI Trend by Intent (Contribution)",
